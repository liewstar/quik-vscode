<Panel>
    <GroupBox title="uiuiucdcdcd">
        <!-- 配置选择器（动态选项） -->
        <ComboBox title="选择配置 来吧来吧" var="selectedConfig" default="0">
            <Choice q-for="(item, idx) in configs" text="$item.name" val="$idx"/>
        </ComboBox>
        
        <!-- 动态表单：根据 selectedConfig 显示对应的表单 -->
        <GroupBox q-for="(form, idx) in forms" title="$form.name 参数" visible="$selectedConfig==$idx">
            <LineEdit var="formData.$idx.name" title="名称"/>
            <DoubleSpinBox var="formData.$idx.value" title="数值" min="0" max="100"/>
            <CheckBox var="formData.$idx.enabled" text="启用"/>
        </GroupBox>
    </GroupBox>
    
    <HLayoutWidget>
        <PushButton text="123123" var="btnAdd"/>
        <PushButton text="z" var="btnRemove"/>
        <PushButton text="打印阿" var="btnPrint"/>
    </HLayoutWidget>
</Panel>
